{
  "file": "Kubernetes_manifest.uvl",
  "config": {
    "Kubernetes_manifest": true,
    "ApiVersion": true,
    "GROUP_apps": true,
    "VERSION_v1": true,
    "Kind": true,
    "KIND_Deployment": true,
    "Metadata": true,
    "METADATA_name": "mi-deployment",
    "METADATA_namespace": "namespace-prueba",
    "METADATA_cluster": "my-cluster",
    "METADATA_labels": [
      {
        "METADATA_labels": true,
        "LABELS_key": "app",
        "LABELS_value": "my-app"
      },
      {
        "METADATA_labels": true,
        "LABELS_key": "key2",
        "LABELS_value": "value2"
      }
    ],
    "METADATA_annotations": [
      {
        "METADATA_annotations": true,
        "ANNOTATIONS_key": "createdBy",
        "ANNOTATIONS_value": "John Doe"
      },
      {
        "METADATA_annotations": true,
        "ANNOTATIONS_key": "description",
        "ANNOTATIONS_value": "This is a sample pod for demonstration purposes."
      }
    ],
    "Spec": true,
    "Containers": true,
    "CONTAINER_image":"nginx:1.14.2",
    "CONTAINER_name":"nginx",
    "Ports":true,
    "PORTS_ContainerPort": 80,
    "CONTAINER_resources": true,
    "RESOURCES_request":true,
    "REQUEST_cpu":"200m",
    "REQUEST_memory":"200Mi",
    "RESOURCES_limits": true,
    "LIMITS_cpu":"500m",
    "LIMITS_memory":"500Mi"
  }
}