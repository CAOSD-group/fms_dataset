          {
            "MATCHEXPRESSIONS_value": "Valor_1"
          },
          {
            "MATCHEXPRESSIONS_value": "Valor_2"
          }
        ]
      },
      {
        "MATCHEXPRESSIONS_key": "Key_2",
        "MATCHEXPRESSIONS_operator": true,
        "OPERATOR_doesNotExist": true
      }
