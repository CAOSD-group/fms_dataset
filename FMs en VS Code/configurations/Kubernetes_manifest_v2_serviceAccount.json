{
  "file": "Kubernetes_manifest_v2_role.uvl",
  "config": {
    "Kubernetes_manifest": true,
    "apiVersion": true,
    "Group": true,
    "GROUP_core": true,
    "Version": true,
    "VERSION_v1": true,
    "kind": true,
    "KIND_ServiceAccount": true,
    "metadata": true,
    "METADATA_name": "ejemplo-ServiceAccount",
    "METADATA_namespace": "default",
    "automountServiceAccountToken": false,
    "imagePullSecrets": [
      {
        "IMAGEPULLSECRETS_name": "custom-my-registry-secret"
      }
    ],
    "secrets": [
      {
        "SECRETS_name": "custom-secret"
      },
      {
        "SECRETS_name": "another-secret"
      }
    ]
  }
}