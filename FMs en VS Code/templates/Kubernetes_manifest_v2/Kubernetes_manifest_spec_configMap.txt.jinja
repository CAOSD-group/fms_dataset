{%if CONFIGMAPSPEC_immutable%}immutable: true{%else%}immutable: false{%endif%}

{%if CONFIGMAPSPEC_kind%}kind: {{CONFIGMAPSPEC_kind}}{%endif%}


{%if CONFIGMAPSPEC_binaryData%}binaryData:
{%for b in CONFIGMAPSPEC_binaryData%}
  {{b.BINARYDATA_binaryFile_name}}: |
    {{b.BINARYDATA_binaryFile_value}}
{%endfor%}
{%endif%}

{%if CONFIGMAPSPEC_data%}data:
{%for d in CONFIGMAPSPEC_data%}
  {%if d.DATA_confSimple%}{{d.CONFSIMPLE_key}}: {{d.CONFSIMPLE_value}}{%endif%}

  {%if d.DATA_confFile%}{{d.CONFFILE_name}}: |
    {{d.CONFFILE_value}}
  {%endif%}
{%endfor%}
{%endif%}
