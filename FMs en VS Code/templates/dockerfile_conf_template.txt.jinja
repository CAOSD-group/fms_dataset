{# Comando From #}
FROM {% if platform %}--platform={{PLATFORM_value}} {% endif %}{{image}}{% if tag %}:{{TAG_value}}{% endif %}{% if digest %}@{{DIGEST_value}}{% endif %}{% if name %} AS {{NAME_value}}{% endif %}
{# Comando WORKDIR #}
{% if WORKDIR %}WORKDIR {{WORKDIR_path}}{% endif %}
{# Comando COPY #}
{% if COPY %}COPY {% if COPY_from%}{{COPY_from}}{{COPY_FROM_options}} {% endif %}{% if COPY_exclude%}{{COPY_exclude}}{{COPY_EXCLUDE_path}} {% endif %}{% if COPY_chown %}{{COPY_chown}}{{COPY_CHOWN_users}}{% if COPY_CHOWN_group %}:{{COPY_CHOWN_GROUP_value}}{% endif %} {% if COPY_chmod %}{{COPY_chmod}}{{COPY_CHMOD_perms}} {% endif %}{% endif %}{% if COPY_link %}{{COPY_link}} {% endif %}{% if COPY_parents %}{{COPY_parents}} {% endif %}{{COPY_source}} {{COPY_destiny}}{% endif %}
{# Comando ADD #}
{% if ADD %}ADD {% if ADD_keep_git_dir %}{{ADD_keep_git_dir}} {% endif %}{% if ADD_checksum %}{{ADD_checksum}}{{ADD_CHECKSUM_value}} {% endif %}{% if ADD_exclude%}{{ADD_exclude}}{{ADD_EXCLUDE_path}} {% endif %}{% if ADD_exclude%}{{ADD_exclude}}{{ADD_EXCLUDE_path}} {% endif %}{% if ADD_chown %}{{ADD_chown}}{{ADD_CHOWN_users}}{% if ADD_CHOWN_group %}:{{ADD_CHOWN_GROUP_value}}{% endif %} {% if ADD_chmod %}{{ADD_chmod}}{{ADD_CHMOD_perms}} {% endif %}{% endif %}{% if ADD_link %}{{ADD_link}} {% endif %}{{ADD_source}} {{ADD_destiny}}{% endif %}
{# Comando LABEL #}
{% if LABEL%}LABEL  {{LABEL_key}}="{{LABEL_value}}"{% endif %}
{# Comando ENV #}
{% if ENV%}ENV  {{ENV_key}}={{ENV_value}}{% endif %}
{# Comando ARG #}
{% if ARG %}ARG {{ARG_name}}{% if ARG_value %}={{ARG_value}}{% endif %} {% endif %}
{# Comando ENTRYPOINT #}
{% if ENTRYPOINT %}ENTRYPOINT {{ENTRYPOINT_command}} {{ENTRYPOINT_param1}} {{ENTRYPOINT_param2}}{% endif %}
{# Comando USER #}
{% if USER %}USER {{USER_name}}{% if USER_group %}:{{USER_group}}{% endif %}{% endif %}
{# Comando ONBUILD #}
{% if ONBUILD %}ONBUILD {{ONBUILD_instruction}}{% endif %}
{# Comando STOPSIGNAL #}
{% if STOPSIGNAL %}STOPSIGNAL {{STOPSIGNAL_signal}} {% endif %}
{# Comando SHELL #}
{% if SHELL %}SHELL [{{SHELL_json}}]{% endif %}
{# Comando EXPOSE #}
{% if EXPOSE %}EXPOSE {% if not EXPOSE_protocol %}{{EXPOSE_port}} {% elif EXPOSE_protocol %}{% if PROTOCOL_udp %}{{EXPOSE_port}}/udp {% endif %}{% if PROTOCOL_tcp %}{{EXPOSE_port}}/tcp{% endif %}{% endif %}{% endif %}
{# Comando RUN #}
{% if RUN %}Run{% if mount %} {{mount}}{{bind}}{{cache}}{{tmpfs}}{{secret}}{{ssh}}{% if MOUNT_target %},{{MOUNT_target}}{{MOUNT_TARGET_value}}{% endif %}{% if MOUNT_source %},{{MOUNT_source}}{{MOUNT_SOURCE_value}}{% endif %}{% if MOUNT_from %},{{MOUNT_from}}{{MOUNT_FROM_value}}{% endif %}{% if MOUNT_readwrite %},{{MOUNT_readwrite}}{% endif %}{% if MOUNT_readonly %},{{MOUNT_readonly}}{% endif %}{% if MOUNT_id %},{{MOUNT_id}}{{MOUNT_ID_value}}{% endif %}{% if MOUNT_required %},{{MOUNT_required}}{% endif %}{% if MOUNT_mode %},{{MOUNT_mode}}{{MOUNT_MODE_value}}{% endif %}{% if MOUNT_uid %},{{MOUNT_uid}}{{MOUNT_UID_value}}{% endif %}{% if MOUNT_gid %},{{MOUNT_gid}}{{MOUNT_GID_value}}{% endif %}{% if MOUNT_size %},{{MOUNT_size}}{{MOUNT_SIZE_value}}{% endif %}{% if MOUNT_sharing %},{{MOUNT_sharing}}{{MOUNT_SHARING_shared}}{{MOUNT_SHARING_private}}{{MOUNT_SHARING_locked}}{% endif %}{% endif %}{% if network %} {{network}}{{default}}{{none_}}{{host}}{% endif %}{% if security %} {{security}}{{sandbox}}{{insecure}}{% endif %} {{RUN_command}}{% endif %}
{# Comando HEALTHCHECK #}
{% if HEALTHCHECK %}HEALTHCHECK{%if HEALTHCHECK_none %} {{HEALTHCHECK_none}}{% elif HEALTHCHECK_options%}{% if HEALTHCHECK_interval %} --interval={{HEALTHCHECK_interval}}{% endif %}{% if HEALTHCHECK_timeout %} --timeout={{HEALTHCHECK_timeout}}{% endif %}{% if HEALTHCHECK_startPeriod%} --start-period={{HEALTHCHECK_startPeriod}}{% endif %}{% if HEALTHCHECK_startInterval %} --start-interval={{HEALTHCHECK_startInterval}}{% endif %}{% if HEALTHCHECK_retries %} --retries={{HEALTHCHECK_retries}}{% endif %} \
    CMD{{HEALTHCHECK_cmdCommand}}{% endif %}{% endif %}
{# Comando VOLUME #}
{% if VOLUME%}VOLUME {{VOLUME_arguments}}{% endif %}
{# Comando CMD #}
{% if CMD %}CMD {{CMD_command}} {{CMD_param1}} {{CMD_param2}} {% endif %}



