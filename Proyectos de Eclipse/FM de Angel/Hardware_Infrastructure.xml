<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extendedFeatureModel>
	<properties>
		<graphics key="autolayoutconstraints" value="false"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showconstraints" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="Hardware Infrastructure">
			<graphics key="collapsed" value="false"/>
			<and mandatory="true" name="Hardware device">
				<graphics key="collapsed" value="false"/>
				<attribute name="ew" type="string" unit=""/>
				<attribute name="Alpha" type="string" unit=""/>
				<attribute name="Disk" type="string" unit=""/>
				<attribute name="RAM" type="string" unit=""/>
				<attribute name="Emax" type="string" unit=""/>
				<attribute name="ID" type="string" unit=""/>
				<or abstract="true" mandatory="true" name="Role">
					<graphics key="collapsed" value="false"/>
					<feature name="Interactive"/>
					<feature name="Computing"/>
				</or>
				<alt abstract="true" mandatory="true" name="Device type">
					<graphics key="collapsed" value="false"/>
					<feature name="Computer"/>
					<feature name="Cloud"/>
					<feature name="Smart lock"/>
					<feature name="IoT gateway"/>
					<feature name="Mobile phone"/>
					<feature name="Smartwatch"/>
					<feature name="Raspberry"/>
					<feature name="Alexa"/>
					<feature name="Google Home"/>
					<feature name="Beacon"/>
					<feature name="Mote">
						<attribute name="Controller device ID" type="string" unit=""/>
					</feature>
					<feature name="Home appliance"/>
				</alt>
				<or mandatory="true" name="Power supply">
					<graphics key="collapsed" value="false"/>
					<feature name="Battery">
						<attribute name="Capacity" type="string" unit=""/>
					</feature>
					<feature name="Electrical network"/>
				</or>
				<or abstract="true" name="Peripherals">
					<graphics key="collapsed" value="false"/>
					<feature name="Screen"/>
					<feature name="Camera"/>
					<feature name="Speaker"/>
					<feature name="Keyboard"/>
					<feature name="Tactile Screen"/>
					<feature name="Microphone"/>
					<feature name="Fingerprint sensor"/>
				</or>
				<or abstract="true" name="Sensing units">
					<graphics key="collapsed" value="false"/>
					<feature name="Humidity"/>
					<feature name="Pressure"/>
					<feature name="Temperature"/>
					<feature name="Proximity"/>
					<feature name="Smoke"/>
					<feature name="Presence"/>
					<feature name="Luminosity"/>
					<feature name="Gyroscope"/>
					<feature name="Dendrometer"/>
				</or>
				<feature name="Virtual">
					<attribute name="ID physical node" type="string" unit=""/>
				</feature>
				<feature name="CPU">
					<attribute name="GHz" type="string" unit=""/>
					<attribute name="N Cores" type="string" unit=""/>
				</feature>
				<feature name="GPU">
					<attribute name="RAM" type="string" unit=""/>
					<attribute name="Core clocks" type="string" unit="MHz"/>
				</feature>
				<and abstract="true" mandatory="true" name="Network connectivity">
					<graphics key="collapsed" value="false"/>
					<attribute name="P(Rx)" type="string" unit=""/>
					<attribute name="P(Tx)" type="string" unit=""/>
					<attribute name="R(Rx)" type="string" unit=""/>
					<attribute name="R(Tx)" type="string" unit=""/>
					<and mandatory="true" name="L1-2 protocol">
						<graphics key="collapsed" value="false"/>
						<alt mandatory="true" name="Type_L1-2 protocol">
							<graphics key="collapsed" value="false"/>
							<feature name="Discovering"/>
							<feature name="Direct"/>
						</alt>
						<alt abstract="true" name="Network communication technologies">
							<graphics key="collapsed" value="false"/>
							<feature name="WiFi 5GHz"/>
							<feature name="WiFi 2'4GHz"/>
							<feature name="Bluethooth"/>
							<feature name="IEEE 802-15-4"/>
							<feature name="Ethernet"/>
							<feature name="BLE_L1-2"/>
							<feature name="Bluethooth multihop">
								<attribute name="N Hops" type="string" unit=""/>
							</feature>
						</alt>
					</and>
					<alt mandatory="true" name="L3 protocol">
						<graphics key="collapsed" value="false"/>
						<feature name="ZigBee"/>
						<feature name="IPv6"/>
						<feature name="IPv4"/>
						<feature name="IPSec"/>
						<feature name="RLP"/>
						<feature name="ICMP"/>
						<feature name="6LoWPAN"/>
					</alt>
					<alt mandatory="true" name="L4 protocol">
						<graphics key="collapsed" value="false"/>
						<feature name="MQTT"/>
						<feature name="HTTP2"/>
						<feature name="ZiBee App"/>
						<feature name="BLE_L4"/>
					</alt>
				</and>
				<or abstract="true" mandatory="true" name="Owner">
					<graphics key="collapsed" value="false"/>
					<feature name="Public"/>
					<feature name="Other">
						<attribute name="Name" type="string" unit=""/>
					</feature>
				</or>
			</and>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<disj>
					<var>Alexa</var>
					<disj>
						<var>Google Home</var>
						<disj>
							<var>Smartwatch</var>
							<var>Home appliance</var>
						</disj>
					</disj>
				</disj>
				<var>Interactive</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<disj>
					<var>Smartwatch</var>
					<var>Smart lock</var>
				</disj>
				<not>
					<var>Ethernet</var>
				</not>
			</imp>
		</rule>
	</constraints>
</extendedFeatureModel>
